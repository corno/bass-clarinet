{
  "name": "bass-clarinet",
  "description": "SAX based evented streaming JSON parser in Typescript (browser and node)",
  "author": "corno",
  "contributors": [
    "Nuno Job <nunojobpinto@gmail.com> (http://nunojob.com/)",
    "Jann Horn (https://github.com/thejh)",
    "Justin Makeig <jmpublic+github@makeig.com> (http://www.audiblepop.com)",
    "Roly Fentanes (http://about.me/roly)",
    "John Lancaster (http://jlank.com)"
  ],
  "version": "0.57.0",
  "main": "./dist/src/index.js",
  "homepage": "https://github.com/corno/bass-clarinet",
  "repository": {
    "type": "git",
    "url": "http://github.com/corno/bass-clarinet.git"
  },
  "bugs": {
    "url": "http://github.com/corno/bass-clarinet/issues"
  },
  "runkitExampleFilename": "example.js",
  "keywords": [
    "sax",
    "json",
    "typescript",
    "comments",
    "parser",
    "stream",
    "streaming",
    "event",
    "tokenizer",
    "events",
    "emitter",
    "async",
    "streamer",
    "browser",
    "tagged union",
    "tagged",
    "union",
    "sum type",
    "sum",
    "type"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.6",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "mocha": "^7.2.0",
    "should": "13.2.x",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*.ts",
    "buildLintAndTest": "npm run build && npm run lint && npm run test",
    "validatePublishReadiness": "git diff --exit-code && ncu -u --packageFile package.json && npm install && npm run buildLintAndTest",
    "pubMin": "npm run validatePublishReadiness && npm version minor && git push && npm publish",
    "pubPatch": "npm run validatePublishReadiness && npm version patch && git push && npm publish",
    "test": "tsc && node ./node_modules/mocha/bin/mocha dist/test/bass_clarinet.js && node ./node_modules/mocha/bin/mocha dist/test/JSONTestSuite.js",
    "testOwnSuite": "tsc && node ./node_modules/mocha/bin/mocha dist/test/bass_clarinet.js",
    "testJSONTestSuite": "tsc && node ./node_modules/mocha/bin/mocha dist/test/JSONTestSuite.js",
    "update2latest": "ncu -u --packageFile package.json && nsi",
    "update2latestAndPublishMinor": "npm run update2latest && git commit -am \"u2l\" && npm run pubMin",
    "update2latestAndPublishPath": "npm run update2latest && git commit -am \"u2l\" && npm run pubPatch"
  },
  "engines": {
    "node": ">=8.0.0",
    "firefox": ">=0.8.0",
    "chrome": ">=16.0.912"
  },
  "directories": {
    "test": "test"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "pareto": "^0.39.0"
  }
}
